{
	"name": "Monokai Kong",
	"type": "dark",
	"tokenColors": [
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#f92672"
			}
		},
		{
			"name": "Storage Type",
			"scope": "storage.type",
			"settings": {
				"foreground": "#c3fa8e"
			}
		},
		{
			"name": "Storage Type of Arrow Functions",
			"scope": "storage.type.function.arrow",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"constant.language",
				"support.type"
			],
			"settings": {
				"foreground": "#fcfc7f"
			}
		},
		{
			"name": "Keyword Operator",
			"scope": [
				"keyword.operator",
				"keyword.control.as",
				"keyword.control.is",
				"meta.brace.square"
			],
			"settings": {
				"foreground": "#c726f8"
			}
		},
		{
			"name": "Keyword Expression Operator",
			"scope": "keyword.operator.expression",
			"settings": {
				"foreground": "#ed6a43",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keyword Ternary Operator",
			"scope": "keyword.operator.ternary",
			"settings": {
				"foreground": "#ed6a43"
			}
		},
		{
			"name": "Keyword New Operator",
			"scope": "keyword.operator.new",
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"name": "Keyword Optional Operator",
			"scope": "keyword.operator.optional",
			"settings": {
				"foreground": "#c3fa8e"
			}
		},
		{
			"name": "Key/Value Separator",
			"scope": [
				"punctuation.separator.key-value",
				"keyword.operator.type.annotation"
			],
			"settings": {
				"foreground": "#f92672"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#5e813b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Comment Param Decorator",
			"scope": "storage.type.class.jsdoc",
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "Comment Param Name",
			"scope": "variable.other.jsdoc",
			"settings": {
				"foreground": "#c3fa8e"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#fc030e"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#00ff00"
			}
		},
		{
			"name": "Begin/End of Embeded String",
			"scope": [
				"punctuation.definition.string.template.begin",
				"punctuation.definition.string.template.end"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Begin/End of Embeded String Expression",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"string.embedded.begin",
				"string.embedded.end"
			],
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
			"name": "Class",
			"scope": [
				"entity.name.class",
				"entity.name.module",
				"entity.name.type",
				"storage.identifier",
				"support.class",
				"constant.language.import-export-all",
				"variable.other.readwrite.alias"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"name": "Inherited Class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Interface",
			"scope": "entity.name.type.interface",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#ed6a43"
			}
		},
		{
			"name": "Function Call",
			"scope": "entity.name.function-call",
			"settings": {
				"foreground": "#abb2bf"
			}
		},
		{
			"name": "Built-In Constant/Variable/Class/Function",
			"scope": [
				"variable.language",
				"support.constant",
				"support.variable",
				"support.class.builtin",
				"support.class.console",
				"support.function",
				"function.support.builtin",
				"function.support.core"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffcc00"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "Object Property Variable",
			"scope": [
				"meta.block variable.other.object.property",
				"text.html.derivative variable.other.object"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"name": "Embed String Variable",
			"scope": [
				"meta.embedded.line punctuation.accessor",
				"meta.embedded.line variable.other.object",
				"meta.embedded.line variable.other.property",
				"meta.embedded.line variable.other.readwrite"
			],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "JSON Dictionary Name",
			"scope": [
				"support.dictionary.json",
				"structure.dictionary.property-name.json"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "JSON Property Name",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#ffcc00"
			}
		},
		{
			"name": "Markup Tag",
			"scope": [
				"entity.name.tag",
				"entity.name.tag.class.js",
				"entity.name.tag.class.jsx"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "Markup Attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#ffcc00"
			}
		},
		{
			"name": "Markup Class/ID Attribute",
			"scope": [
				"entity.name.tag.class",
				"entity.name.tag.id",
				"text.html.derivative meta.attribute.class entity.other.attribute-name",
				"text.html.derivative meta.attribute.id entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#56b6c2"
			}
		},
		{
			"name": "Markup Text",
			"scope": [
				"text.xml",
				"text.html.derivative"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Markup Others",
			"scope": [
				"text.xml meta.tag",
				"text.html.derivative meta.tag",
				"text.html.derivative variable.other.readwrite",
				"text.html.derivative variable.other.property",
				"text.html.derivative punctuation.accessor",
				"text.html.derivative punctuation.separator.key-value"
			],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "Markup Angular",
			"scope": "entity.other.ng-binding-name",
			"settings": {
				"foreground": "#14cc52"
			}
		},
		{
			"name": "Markup Angular Event",
			"scope": "entity.other.ng-binding-name.event",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup Angular Controls",
			"scope": [
				"entity.other.ng-binding-name.ngIf",
				"entity.other.ng-binding-name.ngIfElse",
				"entity.other.ng-binding-name.ngFor",
				"entity.other.ng-binding-name.ngSwitch",
				"entity.other.ng-binding-name.ngModel"
			],
			"settings": {
				"fontStyle": "bold italic"
			}
		},
		{
			"name": "Markup Angular Function Name",
			"scope": [
				"meta.ng-binding entity.name.function",
				"entity.name.function.pipe"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "StyleSheet Tag",
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.scss",
				"entity.name.tag.less",
				"entity.name.tag.sass",
				"entity.other.attribute-name.pseudo-class"
			],
			"settings": {
				"foreground": "#e06c75"
			}
		},
		{
			"name": "StyleSheet Property Name",
